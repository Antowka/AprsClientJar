<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

       <bean id="uc6kfq" class="ru.antowka.aprs.model.AprsClient">
              <property name="callsign" value="UC6KFQ" />
              <property name="pass" value="000000" />
              <property name="latitude" value="5619.09N" />
              <property name="longitude" value="4403.24E" />
              <property name="ico" value="y" />
              <property name="message" value="op.Anton (UC6KFQ/3) 145.500Mhz/433.500Mhz" />
       </bean>

       <bean id="uc6kfqWx" class="ru.antowka.aprs.model.AprsWeather">
              <property name="callsign" value="UC6KFQ-WX" />
              <property name="pass" value="00000" />
              <property name="latitude" value="5619.08N" />
              <property name="longitude" value="04403.24E" />
              <property name="ico" value="_" />
              <property name="message" value="Virtual weather station UC6KFQ-WX" />
              <property name="link" value="http://api.openweathermap.org/data/2.5/weather?id=520555&amp;appid={API-TOKEN}&amp;mode=xml&amp;units=imperial" />
       </bean>
</beans>